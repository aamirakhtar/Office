@{
    Layout = null;
    var employees = (List<Employee>)ViewData["employees"];
    //var employees = (List<Employee>)ViewBag.employees;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <label>@ViewBag.msg</label>
        <table>
            <tr>
                <td>Name</td>
                <td>DepartmentId</td>
                <td>Age</td>
                <td>Designation</td>
                <td>Address</td>
                <td>Gender</td>
            </tr>
            @foreach (var employee in employees)
            {
                <tr>
                    <td>@employee.Name</td>
                    <td>@employee.DepartmentId</td>
                    <td>@employee.Age</td>
                    <td>@employee.Designation</td>
                    <td>@employee.Address</td>
                    <td>@employee.Gender</td>
                </tr>
            }
        </table>
        <br />
        <a href="CreateEmployee">Create Employee</a>
    </div>
</body>
</html>